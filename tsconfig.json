{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include":  ["src", "styled-system"]
}